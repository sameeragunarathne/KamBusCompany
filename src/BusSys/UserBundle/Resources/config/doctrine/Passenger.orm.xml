<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BusSys\UserBundle\Entity\Passenger" table="passenger">
    <id name="id" type="string" column="id" length="6">
      <!--<generator strategy="IDENTITY"/>-->
    </id>
    <field name="lastname" type="string" column="lastname" length="20" nullable="true"/>
    <field name="initials" type="string" column="initials" length="10" nullable="true"/>
    <field name="telephoneNumber" type="string" column="telephone_number" length="10" nullable="true"/>
    <field name="email" type="string" column="email" length="30" nullable="true"/>
  </entity>
</doctrine-mapping>
